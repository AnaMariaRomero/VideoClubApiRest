services:
  videoclubapirest.api:
    build:
      context: C:\Users\kaomeram\source\repos\VideoClubAPIREST
      dockerfile: VideoClubApiRest.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: VideoClubApiRest.Api
      target: base
    container_name: VideoClubApiRest.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: videoclubapirestapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/VideoClubApiRest.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /c/Users/kaomeram/source/repos/VideoClubAPIREST/VideoClubApiRest.Api:/app:rw
    - /c/Users/kaomeram/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/kaomeram/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/kaomeram/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Microsoft/Xamarin/NuGet:/root/.nuget/fallbackpackages:ro
    - /c/Users/kaomeram/.nuget/packages:/root/.nuget/packages:ro
    - /c/Users/kaomeram/source/repos/VideoClubAPIREST:/src:rw
version: '3.4'